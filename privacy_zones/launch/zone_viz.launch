<launch>
	<arg name="map_yaml"/>
	<arg name="zone_yaml"/>
	<arg name="map_server" default="true"/>

	<node name="map_server" type="map_server" pkg="map_server" args="$(arg map_yaml)" if="$(arg map_server)"/>
	<node name="zone_viz" type="zoneViz.py" pkg="privacy_zones" args="$(arg zone_yaml) $(arg map_yaml)"/>
</launch>